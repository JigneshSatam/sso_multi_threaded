<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= @user.name %>
      </h1>
    </section>
    <% if logged_in? %>
      <%= link_to "Log out", logout_authentications_path, method: :delete %>
    <% end %>
  </aside>
</div>
